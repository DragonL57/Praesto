{
    "buildCommand": "pnpm run build",
    "functions": {
        "api/**/*.py": {
            "runtime": "@vercel/python@3.1.0",
            "memory": 1024
        },
        "app/api/get_transcript/route.ts": {
            "memory": 1024,
            "maxDuration": 30
        }
    },
    "routes": [
        {
            "src": "/api/get_transcript",
            "dest": "api/api.py"
        },
        {
            "src": "/api/get_transcript/(.*)",
            "dest": "api/api.py"
        }
    ]
}