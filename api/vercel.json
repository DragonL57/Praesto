{
  "buildCommand": "pnpm run build",
  "functions": {
    "api/**/*.py": {
      "runtime": "@vercel/python@3.1.0",
      "memory": 1024
    },
    "app/api/public/transcript/route.ts": {
      "memory": 1024,
      "maxDuration": 30
    },
    "api/index.py": {
      "runtime": "@vercel/python@3.1.0",
      "memory": 1024,
      "maxDuration": 30
    },
    "api/api.py": {
      "runtime": "@vercel/python@3.1.0",
      "memory": 1024,
      "maxDuration": 30
    }
  },
  "routes": [
    {
      "src": "/api/public/transcript",
      "dest": "/api/api"
    }
  ]
}